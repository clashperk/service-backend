{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/clash-client": "<rootDir>/../libs/clash-client/src",
    "@app/clash-client/(.*)": "<rootDir>/../libs/clash-client/src/$1",
    "@app/discord-client": "<rootDir>/../libs/discord-client/src",
    "@app/discord-client/(.*)": "<rootDir>/../libs/discord-client/src/$1",
    "@app/discord-oauth": "<rootDir>/../libs/discord-oauth/src",
    "@app/discord-oauth/(.*)": "<rootDir>/../libs/discord-oauth/src/$1",
    "@app/google-sheet": "<rootDir>/../libs/google-sheet/src",
    "@app/google-sheet/(.*)": "<rootDir>/../libs/google-sheet/src/$1"
  }
}