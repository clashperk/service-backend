{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noUncheckedIndexedAccess": true,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/entities": [
        "libs/entities/src"
      ],
      "@app/entities/*": [
        "libs/entities/src/*"
      ],
      "@app/constants": [
        "libs/constants/src"
      ],
      "@app/constants/*": [
        "libs/constants/src/*"
      ],
      "@app/mongodb": [
        "libs/mongodb/src"
      ],
      "@app/mongodb/*": [
        "libs/mongodb/src/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/elastic": [
        "libs/elastic/src"
      ],
      "@app/elastic/*": [
        "libs/elastic/src/*"
      ],
      "@app/helper": [
        "libs/helper/src"
      ],
      "@app/helper/*": [
        "libs/helper/src/*"
      ],
      "@app/repositories": [
        "libs/repositories/src"
      ],
      "@app/repositories/*": [
        "libs/repositories/src/*"
      ],
      "@app/clash-client": [
        "libs/clash-client/src"
      ],
      "@app/clash-client/*": [
        "libs/clash-client/src/*"
      ],
      "@app/discord-client": [
        "libs/discord-client/src"
      ],
      "@app/discord-client/*": [
        "libs/discord-client/src/*"
      ],
      "@app/kafka": [
        "libs/kafka/src"
      ],
      "@app/kafka/*": [
        "libs/kafka/src/*"
      ],
      "@app/validators": [
        "libs/validators/src"
      ],
      "@app/validators/*": [
        "libs/validators/src/*"
      ]
    }
  }
}