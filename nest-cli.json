{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/service-cron/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/service-cron/tsconfig.app.json"
  },
  "generateOptions": {
    "spec": false
  },
  "monorepo": true,
  "root": "apps/service-cron",
  "projects": {
    "service-cron": {
      "type": "application",
      "root": "apps/service-cron",
      "entryFile": "main",
      "sourceRoot": "apps/service-cron/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-cron/tsconfig.app.json"
      }
    },
    "service-capital": {
      "type": "application",
      "root": "apps/service-capital",
      "entryFile": "main",
      "sourceRoot": "apps/service-capital/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-capital/tsconfig.app.json"
      }
    },
    "service-clans": {
      "type": "application",
      "root": "apps/service-clans",
      "entryFile": "main",
      "sourceRoot": "apps/service-clans/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-clans/tsconfig.app.json"
      }
    },
    "service-wars": {
      "type": "application",
      "root": "apps/service-wars",
      "entryFile": "main",
      "sourceRoot": "apps/service-wars/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-wars/tsconfig.app.json"
      }
    },
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "constants": {
      "type": "library",
      "root": "libs/constants",
      "entryFile": "index",
      "sourceRoot": "libs/constants/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constants/tsconfig.lib.json"
      }
    },
    "mongodb": {
      "type": "library",
      "root": "libs/mongodb",
      "entryFile": "index",
      "sourceRoot": "libs/mongodb/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongodb/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "elastic": {
      "type": "library",
      "root": "libs/elastic",
      "entryFile": "index",
      "sourceRoot": "libs/elastic/src",
      "compilerOptions": {
        "tsConfigPath": "libs/elastic/tsconfig.lib.json"
      }
    }
  }
}